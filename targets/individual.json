{
    "$id": "https://raw.githubusercontent.com/fovea-research/cacao-json-schema/security-playbooks-v1.0-cs02/schemas/targets/individual.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "individual",
    "description": "This target type is used for commands thatneed to be processed or executed by an individual. This object inherits thecommon target properties. In addition to the inherited properties, this sectiondefines two additional specific properties that are valid for this type.",
    "type": "object",
    "allOf": [
        {
            "$ref": "https://raw.githubusercontent.com/fovea-research/cacao-json-schema/security-playbooks-v1.0-cs02/schemas/targets/target.json"
        },
        {
            "properties": {
                "type": {
                    "type": "string",
                    "description": "The value of this property MUST be individual.",
                    "enum": [
                        "individual"
                    ]
                },
                "contact": {
                    "$ref": "https://raw.githubusercontent.com/fovea-research/cacao-json-schema/security-playbooks-v1.0-cs02/schemas/data-types/contact.json",
                    "description": "Contact information for this target."
                },
                "location": {
                    "$ref": "https://raw.githubusercontent.com/fovea-research/cacao-json-schema/security-playbooks-v1.0-cs02/schemas/data-types/civic-location.json",
                    "description": "Physical address information for this target."
                }
            },
            "required": [
                "type"
            ]
        }
    ],
    "unevaluatedProperties": false
}