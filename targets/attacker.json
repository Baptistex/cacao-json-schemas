{
    "$id": "https://raw.githubusercontent.com/fovea-research/cacao-json-schema/security-playbooks-v1.0-cs02/schemas/targets/attacker.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "attacker",
    "description": "This target type is used for commands thatneed to be executed on an attack orchestration system. This object inherits thecommon target properties. In addition to the inherited properties, this sectiondefines three additional specific properties that are valid for this type.",
    "type": "object",
    "allOf": [
        {
            "$ref": "https://raw.githubusercontent.com/fovea-research/cacao-json-schema/security-playbooks-v1.0-cs02/schemas/targets/target.json"
        },
        {
            "properties": {
                "type": {
                    "type": "string",
                    "description": "The value of this property MUST be attacker.",
                    "enum": [
                        "attacker"
                    ]
                },
                "executor": {
                    "$ref": "https://raw.githubusercontent.com/fovea-research/cacao-json-schema/security-playbooks-v1.0-cs02/schemas/targets/target.json",
                    "description": "The executor of the attack"
                },
                "executor-type": {
                    "type": "string",
                    "description": "The type of orchestration system that runs the attacks. The values for this property MUST come from the orchestration-system-type vocabulary."
                },
                "subject": {
                    "$ref": "https://raw.githubusercontent.com/fovea-research/cacao-json-schema/security-playbooks-v1.0-cs02/schemas/targets/target.json",
                    "description": "The subject of the attack"
                }
            },
            "required": [
                "type"
            ]
        }
    ],
    "unevaluatedProperties": false
}