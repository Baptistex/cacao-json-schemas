{
    "$id": "https://raw.githubusercontent.com/fovea-research/cacao-json-schemas/security-playbooks-v1.0-cs02/schemas/targets/group.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "group",
    "description": "This target type is used for commands thatneed to be processed or executed by a group. This object inherits the commontarget properties. In addition to the inherited properties, this sectiondefines two additional specific properties that are valid for this type.",
    "type": "object",
    "allOf": [
        {
            "$ref": "https://raw.githubusercontent.com/fovea-research/cacao-json-schemas/security-playbooks-v1.0-cs02/schemas/targets/target.json"
        },
        {
            "properties": {
                "type": {
                    "type": "string",
                    "description": "The value of this property MUST be group.",
                    "enum": [
                        "group"
                    ]
                },
                "contact": {
                    "$ref": "https://raw.githubusercontent.com/fovea-research/cacao-json-schemas/security-playbooks-v1.0-cs02/schemas/data-types/contact.json",
                    "description": "Contact information for this target."
                },
                "location": {
                    "$ref": "https://raw.githubusercontent.com/fovea-research/cacao-json-schemas/security-playbooks-v1.0-cs02/schemas/data-types/civic-location.json",
                    "description": "Physical address information for this target."
                }
            },
            "required": [
                "type"
            ]
        }
    ],
    "unevaluatedProperties": false
}