{
    "$id": "https://raw.githubusercontent.com/fovea-research/cacao-json-schema/security-playbooks-v1.0-cs02/schemas/data-types/dictionary.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "dictionary",
    "description": "The dictionary datatype captures an arbitrary set of key/value pairs and uses the JSON object type [RFC8259] for serialization.",
    "type": "object",
    "minProperties": 1,
    "patternProperties": {
        "^[a-zA-Z0-9_-]{0,250}$": {
            "anyOf": [
                {
                    "type": "array",
                    "minItems": 1
                },
                {
                    "type": "string"
                },
                {
                    "type": "integer"
                },
                {
                    "type": "boolean"
                },
                {
                    "type": "number"
                },
                {
                    "type": "object"
                }
            ]
        }
    },
    "additionalProperties": false
}